{
  "manifest_version": 3,
  "name": "Aria2 Indicator",
  "description": "An indicator displaying aria2 download tasks.",
  "version": "1.0",
  "permissions": [
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon/Ar_16.png",
    "32": "icon/Ar_32.png",
    "48": "icon/Ar_48.png",
    "128": "icon/Ar_128.png"
  }
}